#include<iostream>
using namespace std;
int main() {
    int num1 = 0, num2 =1, num3, number;
    cout << "enter the maximum for the range: "<< endl;
    cin >> number;
    while (num1 <= number){
        cout << num1 << endl;
        num3 = num1 + num2;
        num1 = num2;
        num2 = num3;
    }
    return 0;

}
