#include<iostream>
using namespace std;
int main() {
    int sum=0, num[5];
    cout << "enter five numbers in the array: " << endl;
    for(int i=0; i<5; i++){
        cin >> num[i];
        sum = sum + num[i];
    }
    cout << "the sum is " << sum << endl;
    return 0;
    
}
